{
  "version": 2,
  "framework": "vite",
  "buildCommand": "cd quiz-ai && npm install && npm run build",
  "outputDirectory": "quiz-ai/dist",
  "installCommand": "cd quiz-ai && npm install",
  "builds": [
    {
      "src": "quiz-ai/package.json",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/quiz-ai/dist/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/quiz-ai/dist/index.html"
    }
  ]
} 